{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "NODE_ENV=production bun run dist/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.916.0",
    "@aws-sdk/s3-request-presigner": "^3.916.0",
    "@digi-eventos/db": "workspace:*",
    "@elysiajs/bearer": "^1.4.1",
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/openapi": "^1.4.11",
    "dotenv": "catalog:",
    "drizzle-orm": "catalog:",
    "elysia": "catalog:",
    "jose": "^6.1.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/bun": "catalog:",
    "tsdown": "catalog:"
  }
}
